{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Grpc": {
      "Url": "http://0.0.0.0:80",
      "Protocols": "Http2"
    },
    "Https": {
